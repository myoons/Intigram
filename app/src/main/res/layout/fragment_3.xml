<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashActivity"
    android:orientation="vertical"
    android:background="@color/starbucks_background">

    <TextView
        android:id="@+id/Intigram_Title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/container"
        android:layout_marginTop="20dp"
        android:height="90dp"
        android:text="Whose Your Best Friend?"
        android:fontFamily="@font/korean"
        android:textStyle="bold"
        android:textSize="26dp"
        android:gravity="center"
        android:textColor="@android:color/black"/>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:scaleType="fitXY"
        android:background="@drawable/stroke_black"
        android:src="@drawable/me"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18"
        />
    <TextView
        android:id="@+id/textview1"
        android:text="윤서"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView1"
        app:layout_constraintRight_toRightOf="@id/imageView1"
        android:fontFamily="@font/korean"
        app:layout_constraintLeft_toLeftOf="@id/imageView1"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        />

    <View
        android:id = "@+id/line12"
        android:layout_width="1dp"
        android:layout_height="105dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView1"
        app:layout_constraintBottom_toTopOf="@id/imageView2"
        app:layout_constraintLeft_toRightOf="@id/imageView1"
        app:layout_constraintRight_toLeftOf="@id/imageView2"
        android:rotation="-45"
        />

    <View
        android:id = "@+id/line13"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView1"
        app:layout_constraintBottom_toTopOf="@id/imageView3"
        app:layout_constraintLeft_toRightOf="@id/imageView1"
        app:layout_constraintRight_toLeftOf="@id/imageView3"
        android:rotation="-20"
        />

    <View
        android:id = "@+id/line14"
        android:layout_width="1dp"
        android:layout_height="330dp"
        android:background="@color/transparent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/imageView1"
        app:layout_constraintBottom_toTopOf="@id/imageView4"
        app:layout_constraintLeft_toRightOf="@id/imageView1"
        app:layout_constraintRight_toLeftOf="@id/imageView4"
        />

    <View
        android:id="@+id/line15"
        android:layout_width="1dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toTopOf="@id/imageView1"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintRight_toLeftOf="@id/imageView1"
        app:layout_constraintLeft_toRightOf="@id/imageView5"
        android:background="@color/transparent"
        android:rotation="20"
        />

    <View
        android:id="@+id/line16"
        android:background="@color/transparent"
        android:layout_width="1dp"
        android:layout_height="105dp"
        android:rotation="45"
        app:layout_constraintBottom_toTopOf="@id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView1"
        app:layout_constraintRight_toLeftOf="@id/imageView1"
        app:layout_constraintLeft_toRightOf="@id/imageView6"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/person1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35"
        app:layout_constraintHorizontal_bias="0.9"
        />
    <TextView
        android:id="@+id/textview2"
        android:text="@string/nodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView2"
        app:layout_constraintRight_toRightOf="@id/imageView2"
        app:layout_constraintLeft_toLeftOf="@id/imageView2"
        android:textColor="@android:color/black"
        android:fontFamily="@font/korean"
        android:textStyle="bold"
        />

    <View
        android:id = "@+id/line23"
        android:layout_width="1dp"
        android:layout_height="95dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView2"
        app:layout_constraintBottom_toTopOf="@id/imageView3"
        app:layout_constraintLeft_toRightOf="@id/imageView2"
        app:layout_constraintRight_toLeftOf="@id/imageView3"
        app:layout_constraintVertical_bias="0.7"
        />

    <View
        android:id = "@+id/line24"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView2"
        app:layout_constraintBottom_toTopOf="@id/imageView4"
        app:layout_constraintLeft_toLeftOf="@id/imageView2"
        app:layout_constraintRight_toRightOf="@id/imageView4"
        android:rotation="20"
        />

    <View
        android:id = "@+id/line25"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView2"
        app:layout_constraintBottom_toTopOf="@id/imageView5"
        app:layout_constraintLeft_toRightOf="@id/imageView5"
        app:layout_constraintRight_toLeftOf="@id/imageView2"
        android:rotation="50"
        />

    <View
        android:id = "@+id/line26"
        android:layout_width="1dp"
        android:layout_height="200dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView2"
        app:layout_constraintBottom_toTopOf="@id/imageView6"
        app:layout_constraintLeft_toRightOf="@id/imageView6"
        app:layout_constraintRight_toLeftOf="@id/imageView2"
        android:rotation="90"
        />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/person1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.65"
        app:layout_constraintHorizontal_bias="0.9"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/textview3"
        android:text="@string/nodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView3"
        app:layout_constraintRight_toRightOf="@id/imageView3"
        app:layout_constraintLeft_toLeftOf="@id/imageView3"
        android:textColor="@android:color/black"
        android:fontFamily="@font/korean"
        android:textStyle="bold"
        />

    <View
        android:id = "@+id/line34"
        android:layout_width="1dp"
        android:layout_height="105dp"
        android:background="@color/transparent"
        android:rotation="45"
        app:layout_constraintTop_toBottomOf="@id/imageView3"
        app:layout_constraintBottom_toTopOf="@id/imageView4"
        app:layout_constraintLeft_toRightOf="@id/imageView4"
        app:layout_constraintRight_toLeftOf="@id/imageView3"
        />
    <View
        android:id = "@+id/line35"
        android:layout_width="1dp"
        android:layout_height="200dp"
        android:background="@color/transparent"
        android:rotation="90"
        app:layout_constraintTop_toBottomOf="@id/imageView3"
        app:layout_constraintBottom_toTopOf="@id/imageView5"
        app:layout_constraintLeft_toRightOf="@id/imageView5"
        app:layout_constraintRight_toLeftOf="@id/imageView3"

        />

    <View
        android:id = "@+id/line36"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView6"
        app:layout_constraintBottom_toTopOf="@id/imageView3"
        app:layout_constraintLeft_toRightOf="@id/imageView6"
        app:layout_constraintRight_toLeftOf="@id/imageView3"
        android:rotation="-50"
        />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/person1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.82"/>

    <TextView
        android:id="@+id/textview4"
        android:text="@string/nodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        app:layout_constraintRight_toRightOf="@id/imageView4"
        app:layout_constraintLeft_toLeftOf="@id/imageView4"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:fontFamily="@font/korean"
        />
    <View
        android:id = "@+id/line45"
        android:layout_width="1dp"
        android:layout_height="105dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        app:layout_constraintBottom_toTopOf="@id/imageView5"
        app:layout_constraintLeft_toRightOf="@id/imageView5"
        app:layout_constraintRight_toLeftOf="@id/imageView4"
        android:rotation="-45"
        />

    <View
        android:id = "@+id/line46"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        app:layout_constraintBottom_toTopOf="@id/imageView6"
        app:layout_constraintLeft_toRightOf="@id/imageView4"
        app:layout_constraintRight_toLeftOf="@id/imageView6"
        android:rotation="-20"
        />


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/person1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65"
        app:layout_constraintHorizontal_bias="0.1"
        />

    <TextView
        android:id="@+id/textview5"
        android:text="@string/nodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView5"
        app:layout_constraintRight_toRightOf="@id/imageView5"
        app:layout_constraintLeft_toLeftOf="@id/imageView5"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:fontFamily="@font/korean"
        />

    <View
        android:id = "@+id/line56"
        android:layout_width="1dp"
        android:layout_height="100dp"
        android:background="@color/transparent"
        app:layout_constraintTop_toBottomOf="@id/imageView6"
        app:layout_constraintBottom_toTopOf="@id/imageView5"
        app:layout_constraintLeft_toRightOf="@id/imageView6"
        app:layout_constraintRight_toLeftOf="@id/imageView5"
        app:layout_constraintVertical_bias="0.7"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/person1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35"
        app:layout_constraintHorizontal_bias="0.1"/>

    <TextView
        android:id="@+id/textview6"
        android:text="@string/nodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imageView6"
        app:layout_constraintRight_toRightOf="@id/imageView6"
        app:layout_constraintLeft_toLeftOf="@id/imageView6"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:fontFamily="@font/korean"
        />

    <Button
        android:id="@+id/button_accepted"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="100dp"
        android:background="@drawable/arrow2"
        app:layout_constraintLeft_toLeftOf="@id/container" />

    <Button
        android:id="@+id/popup"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintRight_toRightOf="@id/container"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        android:layout_marginRight="100dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/rank" />

</androidx.constraintlayout.widget.ConstraintLayout>